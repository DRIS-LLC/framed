{
  "name": "walker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:clean": "pnpm clean && pnpm dev",
    "build": "next build",
    "build:errors": "tsc --noEmit; next build",
    "clean": "rm -rf node_modules && rm -rf .next && rm -rf .turbo",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm runtime:uncomment && pnpm pages:build && wrangler pages deploy",
    "deploy:prod": "pnpm run deploy --branch=$(node scripts/get-branch.js) && pnpm runtime:comment",
    "types": "wrangler types",
    "logs": "wrangler pages deployment tail",
    "generate": "prisma generate",
    "create:db": "wrangler d1 create $(node scripts/get-db.js)",
    "create:bucket": "wrangler r2 bucket create $(node scripts/get-bucket.js)",
    "migrate": "node scripts/migrate.js",
    "migrate:remote": "node scripts/migrate.js --remote",
    "migrate:apply": "node scripts/migrate.js --apply",
    "migrate:initial": "node scripts/migrate.js --initial",
    "migrate:clear": "node scripts/migrate.js --clear",
    "list": "wrangler d1 migrations list $(node scripts/get-db.js)",
    "list:remote": "wrangler d1 migrations list $(node scripts/get-db.js) --remote",
    "apply": "wrangler d1 migrations apply $(node scripts/get-db.js) && pnpm run generate",
    "apply:remote": "wrangler d1 migrations apply $(node scripts/get-db.js) --remote && pnpm run generate",
    "format": "prettier --write .",
    "runtime:comment": "node scripts/toggle-runtime.js comment",
    "runtime:uncomment": "node scripts/toggle-runtime.js uncomment"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.16.0",
    "@clerk/themes": "^2.2.34",
    "@hookform/resolvers": "^5.0.1",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.3.1",
    "@prisma/adapter-d1": "^6.6.0",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-accordion": "^1.2.7",
    "@radix-ui/react-avatar": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.9",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@tailwindcss/cli": "^4.1.4",
    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "encoding": "^0.1.13",
    "framer-motion": "^12.7.4",
    "lucide-react": "^0.485.0",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "openai": "^4.95.1",
    "path": "^0.12.7",
    "prisma": "^6.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.3",
    "stream": "^0.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.25.0",
    "eslint-config-next": "15.2.1",
    "eslint-plugin-next-on-pages": "^1.13.12",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "wrangler": "^4.12.0"
  }
}
